local WindUI
local function safeLoadWindUI()
    local success, module = pcall(function()
        return loadstring(game:HttpGet("http://localhost:8642/dist/main.lua"))()
    end)
    if success and module then return module end
    -- Fallback to GitHub release
    local ok, res = pcall(function()
        return loadstring(game:HttpGet("https://github.com/Footagesus/WindUI/releases/latest/download/main.lua"))()
    end)
    if ok and res then return res end
    return nil
end

WindUI = safeLoadWindUI()
if not WindUI then
    error("WindUI failed to load. Start local build or ensure internet available.")
end

-- Basic services
local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local UserInputService = game:GetService("UserInputService")
local HttpService = game:GetService("HttpService")
local TeleportService = game:GetService("TeleportService")
local VirtualUser = game:GetService("VirtualUser")
local Workspace = game:GetService("Workspace")

local LocalPlayer = Players.LocalPlayer
local Character, Humanoid, RootPart
local function updateChar(c)
    Character = c
    Humanoid = c and c:FindFirstChildOfClass("Humanoid")
    RootPart = c and c:FindFirstChild("HumanoidRootPart")
end
if LocalPlayer.Character then updateChar(LocalPlayer.Character) end
LocalPlayer.CharacterAdded:Connect(updateChar)

-- State table
local state = {
    fly = false, flySpeed = 50,
    speed = 16, speedHack = false,
    noclip = false,
    infiniteJump = false,
    highJump = false,
    antifling = false,
    hiddenFling = false,
    esp = false, espBoxes = false, espNames = true,
    aimbot = false, aimbotFOV = 60, aimbotSmooth = 10, aimbotTeamCheck = true,
    autoRejoin = false,
    antiAfk = false,
    clickTp = false,
    configName = "default",
}

-- Helpers
local function notify(title, content, dur)
    WindUI:Notify({ Title = title or "GPT HUB", Content = content or "", Duration = dur or 3 })
end

-- Movement: Speed updater
local function applySpeed()
    if Humanoid then
        Humanoid.WalkSpeed = (state.speedHack and state.speed) and state.speed or 16
    end
end
LocalPlayer.CharacterAdded:Connect(function()
    wait(0.5)
    updateChar(LocalPlayer.Character)
    applySpeed()
end)

-- Infinite Jump
local jumpConn
if state.infiniteJump then
    -- placeholder
end
UserInputService.JumpRequest:Connect(function()
    if state.infiniteJump and Humanoid then
        Humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
    end
end)

-- Fly (BodyVelocity + BodyGyro) - generic
local flying = false
local flyBV, flyBG, flyConn
local function startFly()
    if not RootPart or not Humanoid then return end
    if flying then return end
    flying = true
    flyBV = Instance.new("BodyVelocity")
    flyBV.MaxForce = Vector3.new(1e9,1e9,1e9)
    flyBV.Parent = RootPart
    flyBG = Instance.new("BodyGyro")
    flyBG.MaxTorque = Vector3.new(1e9,1e9,1e9)
    flyBG.Parent = RootPart
    Humanoid.PlatformStand = true

    flyConn = RunService.RenderStepped:Connect(function()
        if not flying or not RootPart then return end
        local cam = workspace.CurrentCamera
        local move = Vector3.new(0,0,0)
        local speed = math.max(1, state.flySpeed)
        local dir = Humanoid.MoveDirection
        if dir.Magnitude > 0 then
            move = (cam.CFrame:VectorToWorldSpace(dir)) * speed
        end
        if UserInputService:IsKeyDown(Enum.KeyCode.Space) then move = move + Vector3.new(0, speed, 0) end
        if UserInputService:IsKeyDown(Enum.KeyCode.LeftShift) then move = move - Vector3.new(0, speed, 0) end
        flyBV.Velocity = move
        flyBG.CFrame = cam.CFrame
    end)
end
local function stopFly()
    flying = false
    if flyConn then flyConn:Disconnect(); flyConn = nil end
    if flyBV then flyBV:Destroy(); flyBV = nil end
    if flyBG then flyBG:Destroy(); flyBG = nil end
    if Humanoid then Humanoid.PlatformStand = false end
end

-- Noclip
local noclipConn
local function setNoclip(val)
    if val then
        noclipConn = RunService.Stepped:Connect(function()
            if Character then
                for _, part in ipairs(Character:GetDescendants()) do
                    if part:IsA("BasePart") then part.CanCollide = false end
                end
            end
        end)
    else
        if noclipConn then noclipConn:Disconnect(); noclipConn = nil end
        if Character then
            for _, part in ipairs(Character:GetDescendants()) do
                if part:IsA("BasePart") then part.CanCollide = true end
            end
        end
    end
end

-- Anti-AFK
local afkConn
local function setAntiAfk(val)
    if val then
        afkConn = VirtualUser
        local bg = game:GetService("Players").LocalPlayer.Idled
        -- usual pattern
        LocalPlayer.Idled:Connect(function()
            VirtualUser:CaptureController()
            VirtualUser:ClickButton2(Vector2.new(0,0))
        end)
    else
        -- can't disconnect default Idled listeners above; leaving passive
    end
end

-- ESP - simple highlight for players
local highlights = {}
local function createESPForPlayer(p)
    if p == LocalPlayer then return end
    local chr = p.Character
    if not chr then return end
    if highlights[p] then return end
    local hrp = chr:FindFirstChild("HumanoidRootPart") or chr:FindFirstChildWhichIsA("BasePart")
    if not hrp then return end
    local h = Instance.new("Highlight")
    h.Name = "GPT_ESP"
    h.Adornee = chr
    h.DepthMode = Enum.HighlightDepthMode.Occluded
    h.OutlineTransparency = 0
    h.FillTransparency = 0.7
    h.Parent = workspace
    highlights[p] = h
end
local function removeESPForPlayer(p)
    local h = highlights[p]
    if h then h:Destroy() end
    highlights[p] = nil
end

local function updateESP(val)
    if val then
        for _, p in ipairs(Players:GetPlayers()) do
            if p.Character then createESPForPlayer(p) end
        end
        Players.PlayerAdded:Connect(function(p) if state.esp then createESPForPlayer(p) end end)
        Players.PlayerRemoving:Connect(function(p) removeESPForPlayer(p) end)
    else
        for p,h in pairs(highlights) do
            if h then h:Destroy() end
        end
        highlights = {}
    end
end

-- Aimbot (generic target nearest to crosshair)
local function getAimbotTarget()
    local cam = workspace.CurrentCamera
    local best, bestDist = nil, math.huge
    for _, p in pairs(Players:GetPlayers()) do
        if p ~= LocalPlayer and p.Character and p.Character:FindFirstChild("Humanoid") and p.Character.Humanoid.Health > 0 then
            if state.aimbotTeamCheck then
                local lpTeam = LocalPlayer.Team
                if lpTeam and p.Team == lpTeam then continue end
            end
            local head = p.Character:FindFirstChild("Head") or p.Character:FindFirstChild("HumanoidRootPart")
            if not head then continue end
            local screenPos, onScreen = workspace.CurrentCamera:WorldToViewportPoint(head.Position)
            if not onScreen then continue end
            local center = Vector2.new(workspace.CurrentCamera.ViewportSize.X/2, workspace.CurrentCamera.ViewportSize.Y/2)
            local dist = (Vector2.new(screenPos.X, screenPos.Y) - center).Magnitude
            if dist < bestDist and dist <= state.aimbotFOV then
                bestDist = dist
                best = p
            end
        end
    end
    return best
end

local aimbotConn
local function setAimbot(val)
    if val then
        aimbotConn = RunService.RenderStepped:Connect(function(dt)
            if not workspace.CurrentCamera or not LocalPlayer.Character then return end
            local target = getAimbotTarget()
            if target and target.Character and target.Character:FindFirstChild("Head") then
                local head = target.Character.Head
                local cam = workspace.CurrentCamera
                local desiredCFrame = CFrame.new(cam.CFrame.Position, head.Position)
                local smooth = math.max(1, state.aimbotSmooth)
                cam.CFrame = cam.CFrame:Lerp(desiredCFrame, dt * smooth)
            end
        end)
    else
        if aimbotConn then aimbotConn:Disconnect(); aimbotConn = nil end
    end
end

-- Fling (simple instantaneous velocity blast on target when toggled)
local function doFling(targetPlayer)
    if not targetPlayer or not targetPlayer.Character then return end
    if not RootPart then return end
    local hrp = targetPlayer.Character:FindFirstChild("HumanoidRootPart")
    if not hrp then return end
    -- CFrame teleport near them + force
    local oldPos = RootPart.CFrame
    pcall(function()
        RootPart.CFrame = hrp.CFrame * CFrame.new(0, 3, 0)
        RootPart.Velocity = Vector3.new(0, 500, 0)
        wait(0.05)
    end)
    RootPart.CFrame = oldPos
end

-- Server tools
local function rejoin()
    TeleportService:Teleport(game.PlaceId, LocalPlayer)
end

local function serverHop()
    local succ, res = pcall(function()
        return HttpService:JSONDecode(game:HttpGet("https://games.roblox.com/v1/games/"..game.PlaceId.."/servers/Public?sortOrder=Asc&limit=100"))
    end)
    if succ and res and res.data and #res.data > 0 then
        local pick = res.data[math.random(1, #res.data)]
        if pick and pick.id then
            pcall(function() TeleportService:TeleportToPlaceInstance(game.PlaceId, pick.id, LocalPlayer) end)
        end
    else
        notify("ServerHop", "Failed to fetch servers", 3)
    end
end

-- Click TP
local clickTpConn
local function setClickTP(val)
    if val then
        clickTpConn = UserInputService.InputBegan:Connect(function(inp, gpe)
            if gpe then return end
            if inp.UserInputType == Enum.UserInputType.MouseButton1 then
                local mouse = LocalPlayer:GetMouse()
                local target = mouse.Hit
                if target and RootPart then
                    RootPart.CFrame = CFrame.new(target.p + Vector3.new(0,3,0))
                end
            end
        end)
    else
        if clickTpConn then clickTpConn:Disconnect(); clickTpConn = nil end
    end
end

-- ConfigManager support (WindUI)
local configMgr = WindUI.ConfigManager
local function saveConfig(name)
    if not configMgr then notify("Config", "No ConfigManager available", 3); return end
    local cfg = configMgr:CreateConfig(name or state.configName)
    -- Register controls by key names for persistence
    cfg.Data = {
        fly = state.fly, flySpeed = state.flySpeed,
        speedHack = state.speedHack, speed = state.speed,
        noclip = state.noclip,
        infiniteJump = state.infiniteJump,
        aimbot = state.aimbot, aimbotSmooth = state.aimbotSmooth, aimbotFOV = state.aimbotFOV,
        esp = state.esp,
    }
    cfg:Save()
    notify("Config", "Saved: "..(name or state.configName), 2)
end
local function loadConfig(name)
    if not configMgr then notify("Config", "No ConfigManager available", 3); return end
    local cfg = configMgr:GetConfig(name or state.configName)
    if not cfg then notify("Config", "Config not found", 2); return end
    local d = cfg.Data or {}
    -- apply
    state.fly = d.fly or state.fly
    state.flySpeed = d.flySpeed or state.flySpeed
    state.speedHack = d.speedHack or state.speedHack
    state.speed = d.speed or state.speed
    state.noclip = d.noclip or state.noclip
    state.infiniteJump = d.infiniteJump or state.infiniteJump
    state.aimbot = d.aimbot or state.aimbot
    state.aimbotSmooth = d.aimbotSmooth or state.aimbotSmooth
    state.aimbotFOV = d.aimbotFOV or state.aimbotFOV
    state.esp = d.esp or state.esp
    notify("Config", "Loaded: "..(name or state.configName), 2)
end

