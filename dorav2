Tabs.Utilities:Section({ Title = "Utilities" })
Tabs.Utilities:Toggle({
    Title = "Anti-AFK",
    Value = state.antiAfk,
    Callback = function(v) state.antiAfk = v; setAntiAfk(v) end
})
Tabs.Utilities:Toggle({
    Title = "Click Teleport",
    Value = state.clickTp,
    Callback = function(v) state.clickTp = v; setClickTP(v) end
})
Tabs.Utilities:Button({ Title = "Copy Player List", Callback = function()
    local names = {}
    for _,p in pairs(Players:GetPlayers()) do table.insert(names, p.Name) end
    setclipboard(table.concat(names, ", "))
    notify("Utilities", "Player list copied to clipboard", 2)
end})
Tabs.Utilities:Button({ Title = "Server Hop (fast)", Callback = serverHop })
Tabs.Utilities:Button({ Title = "Rejoin", Callback = rejoin })

-- Settings Tab
Tabs.Settings:Section({ Title = "Config" })
Tabs.Settings:Input({
    Title = "Config name",
    Placeholder = "default",
    Callback = function(v) if v ~= "" then state.configName = v end end
})
Tabs.Settings:Button({
    Title = "Save Config",
    Callback = function() saveConfig(state.configName) end
})
Tabs.Settings:Button({
    Title = "Load Config",
    Callback = function() loadConfig(state.configName) end
})
Tabs.Settings:Divider()
Tabs.Settings:Button({ Title = "Unload GPT HUB", Callback = function()
    notify("GPT HUB", "Unloading..."); wait(0.6)
    pcall(function() Window:Destroy() end)
end})

-- Cleanup on close (ensure we stop running connections)
Window:OnClose(function()
    stopFly()
    setNoclip(false)
    setAimbot(false)
    setClickTP(false)
    updateESP(false)
    -- other cleanup
end)

notify("GPT HUB", "Loaded.", 4)
